pipeline {
     agent any
     stages {
         stage('SCM Checkout') {
              steps {
                 git(branch: 'master', credentialsId: 'eldiopo',  url: 'https://github.com/eldiopo1999/demo.git')
              }
         }
		 stage('Build') {
               steps{
				sh "mvn clean verify"
			  }
         }
	
}
}
